<div class="row text-center">
    <div class="col-md-6">
        <button class=" btn btn-secondary" @onclick="Cancel" style="min-width:120px;">Go Back</button>
    </div>
    <div class="col-md-6">
        <button class=" btn btn-danger" @onclick="Continiue" style="min-width:120px;">Continiue</button>
    </div>
</div>
@code {
    [CascadingParameter] BlazoredModalInstance BlazoredModal { get; set; } = default!;


    private async Task Continiue() => await BlazoredModal.CloseAsync();
    private async Task Cancel() => await BlazoredModal.CancelAsync();
}
